{
  "require": {
    "barrelstrength/sprout-encode-email": "2.0.7",
    "craftcms/cms": "3.5.11.1",
    "craftcms/redactor": "2.7.5",
    "ether/seo": "3.6.6",
    "fortrabbit/craft-copy": "1.0.0-RC12",
    "sebastianlenz/linkfield": "1.0.23",
    "spicyweb/craft-neo": "2.8.10.1",
    "studioespresso/craft-dumper": "2.0.0",
    "topshelfcraft/wordsmith": "3.3.0.1",
    "verbb/super-table": "2.6.2",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
